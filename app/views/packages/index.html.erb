<p style="color: green"><%= notice %></p>

<h1>Packages</h1>

<h3>
  <%= link_to "New package", new_package_path %>
</h3>

<div id="packages">
  <% @packages.each do |package| %>
    <hr>
    <%= render package %>
    <p>
      <%= link_to "Show this package", package %>
    </p>
    <hr>
  <% end %>
</div>

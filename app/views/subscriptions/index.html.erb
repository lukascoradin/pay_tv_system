<p style="color: green"><%= notice %></p>

<h1>Subscriptions</h1>

<h3>
  <%= link_to "New subscription", new_subscription_path %>
</h3>

<div id="subscriptions">
  <% @subscriptions.each do |subscription| %>
    <hr>
    <%= render subscription %>
    <p>
      <%= link_to "Show this subscription", subscription %>
    </p>
    <hr>
  <% end %>
</div>